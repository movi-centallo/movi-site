---
import { contentfulClient } from "../lib/contentful";
import Event from "../components/event.astro";
import type Eventi from "../lib/types.ts"

const entries = await contentfulClient.getEntries<Eventi>({
  content_type: "eventi",
});


---
<body>
  {entries.items.map((item) => (
    
    <Event entry={item} />
  ))}
</body>